<h1>Public Wikis</h1>
  <%# if @wiki %>
<% @wikis.each do |wiki| %>
  <div class="media">
    <div class="media-body">
      <h3 class="media-heading">
        <%= link_to wiki.title, wiki %></h3>
        <small><%= truncate(wiki.body, length: 180, separator: '.') %></small>
    </div>
  </div>
<% end %>
<%#= will_paginate @wikis %>
<%# end %>